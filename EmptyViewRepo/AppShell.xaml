<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="EmptyViewRepo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:EmptyViewRepo"
    xmlns:pages="clr-namespace:EmptyViewRepo.Pages">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="EmptyView Text"
        ContentTemplate="{DataTemplate pages:NotClickableEmptyView}"/>

</Shell>
